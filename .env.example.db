# Example DB env (do NOT commit real credentials)
# Copy to .env.local and fill with your values for local testing
SUPABASE_DB_URL=postgres://user:password@db-host:5432/dbname
# or
POSTGRES_URL=postgres://user:password@db-host:5432/dbname

# Optional: enable DB in development (overrides hasDb checks)
ENABLE_DB_IN_DEV=1

# Optional: enable verbose DB logs to debug query issues
DB_VERBOSE_LOG=1
