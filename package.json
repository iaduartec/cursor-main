{
    "name": "duartec-web",
    "version": "1.0.0",
    "private": true,
    "description": "Sitio web profesional de Duartec Instalaciones Inform치ticas - Soluciones integrales en inform치tica, videovigilancia, sonido y electricidad en Burgos",
    "keywords": [
        "inform치tica",
        "videovigilancia",
        "sonido",
        "electricidad",
        "burgos",
        "instalaciones",
        "nextjs",
        "typescript"
    ],
    "author": "Duartec Instalaciones Inform치ticas",
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "preinstall": "node scripts/guard-pm.mjs",
        "prebuild": "node scripts/prebuild.mjs",
        "build": "pnpm db:migrate && next build",
        "start": "next start",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix --max-warnings=0",
        "analyze": "cross-env ANALYZE=true next build",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "test:e2e": "playwright test",
        "postbuild": "next-sitemap",
        "images:generate": "node scripts/generate_missing_images_runner.js --only-missing --min-bytes 1000",
        "images:check": "node scripts/generate_missing_images_runner.js --only-missing --min-bytes 1000 --dry-run",
        "images:ai": "python tools/generate_ai_images.py --only-missing --style photographic --accent blue",
        "images:ai:force": "python tools/generate_ai_images.py --force --style photographic --accent blue",
        "gen:article": "python tools/generate_article.py --topic",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "tsx --require dotenv/config scripts/db/migrate-supabase.ts",
        "db:seed": "tsx scripts/db/seed-from-mdx.ts",
        "db:seed:projects": "tsx scripts/db/seed-projects.ts",
        "db:seed:services": "tsx scripts/db/seed-services.ts",
        "db:seed:streams": "tsx scripts/db/seed-streams.ts"
    },
    "dependencies": {
        "@neondatabase/serverless": "^1.0.1",
        "@next/bundle-analyzer": "^15.0.0",
        "@stackframe/stack": "^2.8.39",
        "@svgr/webpack": "^8.1.0",
        "@types/react-dom": "^18.3.0",
        "@vercel/analytics": "^1.5.0",
        "@vercel/edge-config": "^1.4.0",
        "contentlayer": "^0.3.4",
        "critters": "^0.0.23",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.44.5",
        "framer-motion": "^10.0.0",
        "gray-matter": "^4.0.3",
        "leaflet": "^1.9.4",
        "lucide-react": "^0.263.0",
        "marked": "^12.0.2",
        "next": "^15.5.2",
        "next-contentlayer": "^0.3.4",
        "next-intl": "^3.0.0",
        "next-seo": "^6.0.0",
        "next-sitemap": "^4.0.0",
        "postcss": "^8.1.0",
        "postgres": "^3.4.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-leaflet": "^4.2.1",
        "sharp": "^0.33.4",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^8.57.1",
        "@playwright/test": "1.55.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.16",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^14.3.1",
        "@types/jest": "^29.0.0",
        "@types/leaflet": "^1.9.20",
        "@types/node": "22",
        "@types/react": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^8.6.0",
        "@typescript-eslint/parser": "^8.6.0",
        "autoprefixer": "^10.4.21",
        "cross-env": "^7.0.3",
        "drizzle-kit": "^0.31.4",
        "esbuild": "0.18.20",
        "eslint": "^8.57.1",
        "eslint-config-next": "^15.5.2",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "husky": "^8.0.0",
        "jest": "^29.0.0",
        "jest-environment-jsdom": "^29.0.0",
        "lint-staged": "^15.0.0",
        "playwright": "^1.40.0",
        "prettier": "^3.0.0",
        "tsx": "^4.19.0"
    },
    "engines": {
        "node": "22.x"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "pnpm": {
        "overrides": {
            "@contentlayer/*>esbuild": "0.18.20",
            "@contentlayer/*>@opentelemetry/api": "1.4.1",
            "@opentelemetry/semantic-conventions": "1.28.0"
        }
    },
    "packageManager": "pnpm@10.0.0"
}