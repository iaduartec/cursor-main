# Vercel Postgres (Dashboard -> Storage -> Postgres -> Connect -> Environment Variables)
# Copy these to your Vercel Project Environment and/or your local .env

POSTGRES_URL="postgres://username:password@host:5432/dbname"
POSTGRES_URL_NON_POOLING="postgres://username:password@host:5432/dbname"
POSTGRES_PRISMA_URL="postgres://username:password@host:5432/dbname"
POSTGRES_USER="username"
POSTGRES_HOST="host"
POSTGRES_PASSWORD="password"
POSTGRES_DATABASE="dbname"

# Fallback for tools expecting DATABASE_URL
DATABASE_URL="${POSTGRES_URL}"

