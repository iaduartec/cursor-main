{
  "name": "duartec-web",
  "version": "1.0.0",
  "private": true,
  "description": "Sitio web profesional de Duartec Instalaciones Inform치ticas - Soluciones integrales en inform치tica, videovigilancia, sonido y electricidad en Burgos",
  "keywords": [
    "inform치tica",
    "videovigilancia",
    "sonido",
    "electricidad",
    "burgos",
    "instalaciones",
    "nextjs",
    "typescript"
  ],
  "author": "Duartec Instalaciones Inform치ticas",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env SKIP_CONTENTLAYER=1 SKIP_DB=1 next dev",
    "preinstall": "node scripts/guard-pm.mjs",
    "prebuild": "node scripts/prebuild.mjs",
    "build": "cross-env SKIP_DB=1 next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest run --reporter verbose",
    "test:run": "vitest run --reporter verbose",
    "test:e2e": "playwright test",
    "postbuild": "next-sitemap",
    "images:generate": "node scripts/generate_missing_images_runner.js --only-missing --min-bytes 1000",
    "images:check": "node scripts/generate_missing_images_runner.js --only-missing --min-bytes 1000 --dry-run",
    "images:ai": "python tools/generate_ai_images.py --only-missing --style photographic --accent blue",
    "images:ai:force": "python tools/generate_ai_images.py --force --style photographic --accent blue",
    "validate:mdx": "node scripts/validate-mdx-frontmatter.cjs",
    "gen:article": "python tools/generate_article.py --topic",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node scripts/db/migrate-simple.js",
    "db:migrate:apply": "tsx scripts/db/migrate.ts",
    "db:seed": "tsx scripts/db/seed-from-mdx.ts",
    "db:seed:projects": "tsx scripts/db/seed-projects.ts",
    "db:seed:services": "tsx scripts/db/seed-services.ts",
    "db:seed:streams": "tsx scripts/db/seed-streams.ts",
    "db:seed:comments:apply": "tsx scripts/db/seed-comments.ts",
    "db:apply": "pnpm run db:migrate:apply && pnpm run db:seed:comments:apply",
    "lint:changed": "node scripts/lint-changed.mjs",
    "lint:staged": "node scripts/lint-staged.mjs",
    "deps:check": "node scripts/dependency-monitor.mjs",
    "deps:update-safe": "pnpm update --latest --workspace-root"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@next/bundle-analyzer": "^15.5.3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/speed-insights": "^1.2.0",
    "contentlayer": "^0.3.4",
    "critters": "^0.0.25",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.5",
    "framer-motion": "^12.23.13",
    "gray-matter": "^4.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "marked": "^16.3.0",
    "next": "^15.5.3",
    "next-contentlayer": "^0.3.4",
    "next-intl": "^4.3.9",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^4.2.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^8.57.1",
    "@playwright/test": "1.55.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "esbuild": "0.18.20",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "10.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "overrides": {
      "@contentlayer/*>esbuild": "0.18.20",
      "@contentlayer/*>@opentelemetry/api": "1.4.1",
      "@opentelemetry/semantic-conventions": "1.28.0"
    }
  },
  "packageManager": "pnpm@10.16.1"
}
