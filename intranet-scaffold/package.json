{
  "name": "duartec-intranet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm db:migrate && next build",
    "start": "next start",
    "db:migrate": "tsx --require dotenv/config scripts/db/migrate-supabase.ts",
    "db:seed": "tsx scripts/db/seed-from-mdx.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^15.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "drizzle-orm": "^0.44.5",
    "postgres": "^3.4.7",
    "@supabase/supabase-js": "^2.57.2",
    "contentlayer": "^0.3.4",
    "gray-matter": "^4.0.3",
    "dotenv": "^16.4.5",
    "tsx": "^4.19.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "@contentlayer/*>esbuild": "0.18.20"
    }
  }
}
